{% for block in section.blocks %}
{% endfor %}
<div class="choko-banner" style="background-color:{{ section.settings.section_background_color }};">
  <div class="container">
    <div class="row">
      <div class="col">
        <div class="img-overlay">
          {% for block in section.blocks %}
            <span class="overlay-img">
              <img src="{{ block.settings.image | img_url: '100x' }}" alt="">
            </span>
          {% endfor %}
        </div>
        <div class="content">
          <div class="logo">
            {% if section.settings.logo_image %}
              <img
                src="{{ section.settings.logo_image | img_url: '100x' }}"
                alt="{{ section.settings.logo_image_alt }}"
              >
            {% endif %}
          </div>
          <div class="main-heading">
            <h2>{{ section.settings.main_heading }}</h2>
          </div>
          <p>{{ section.settings.pragraph }}</p>
          <div class="img">
            {% if section.settings.main_image %}
              <img
                src="{{ section.settings.main_image | img_url: '400x' }}"
                alt="{{ section.settings.main_image_alt }}"
              >
            {% endif %}
          </div>
          <div class="site-btn">
            <a href="{{ section.settings.button_link }}">{{ section.settings.button_text }}</a>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>

{% schema %}
{
  "name": "choko banner",
  "settings": [
      {
      "type": "color",
      "id": "section_background_color",
      "label": "Section Background Color"
    },
    {
      "type": "image_picker",
      "id": "overlay_image",
      "label": "Overlay Image"
    },
    {
      "type": "text",
      "id": "overlay_image_alt",
      "label": "Overlay Image Alt Text"
    },
    {
      "type": "image_picker",
      "id": "logo_image",
      "label": "Logo Image"
    },
    {
      "type": "text",
      "id": "logo_image_alt",
      "label": "Logo Image Alt Text"
    },
    {
      "type": "text",
      "id": "main_heading",
      "label": "Main Heading"
    },
    {
      "type": "textarea",
      "id": "pragraph",
      "label": "Pragraph"
    },
    {
      "type": "image_picker",
      "id": "main_image",
      "label": "Main Image"
    },
    {
      "type": "text",
      "id": "main_image_alt",
      "label": "Main Image Alt Text"
    },
    {
      "type": "url",
      "id": "button_link",
      "label": "Button Link"
    },
    {
      "type": "text",
      "id": "button_text",
      "label": "Button Text"
    }
  ],
  "blocks": [
    {
      "type": "image_block",
      "name": "Image Block",
      "settings": [
        {
          "type": "image_picker",
          "id": "image",
          "label": "add images"
        }
      ]
    }
  ],
  "presets": [
    {
      "name": "Choko Banner"
    }
  ]
}
{% endschema %}
