{% style %}
          .choko-banner {
          text-align: center;
          position: relative;
          padding: 100px 0;
          }
          
          .choko-banner .img-overlay span:nth-child(1) {
          left: 154px;
          }
          
          .choko-banner .img-overlay span:nth-child(1) {
          left: 154px;
          }
          .choko-banner .img-overlay span:nth-child(2) {
          left: 154px;
          bottom: 184px;
          }
          .choko-banner .img-overlay span:nth-child(3) {
          left: 154px;
          right: -58%;
          }
          .choko-banner .img-overlay span:nth-child(4) {
          left: 9%;
          right: -68%;
          top: 39%;
          }
          .choko-banner .img-overlay span:nth-child(5) {
          left: 154px;
          right: -58%;
          top: 64%;
          }
          
          .choko-banner  .img-overlay span {
          position: absolute;
          animation: bannerbouns 2s infinite linear;
          }
          
          .choko-banner h2 {
          text-transform: uppercase;
          color: #fff;
          font-weight: 600;
          text-align: center;
          font-size: 60px;
          width: 100%;
          max-width: 792px;
          margin: auto;
          }
          
          .site-btn a {
          font-size: 16px;
          text-decoration: none;
          background-color: #171140;
          border-radius: 11px;
          padding: 17px 90px;
          text-transform: uppercase;
          display: inline-block;
          font-weight: 700;
          border:none !important;
          color: #fff;
          margin-top: 12px;
          transition: all 0.2s linear;
          }
          
          .site-btn a:hover {
          background-color: #F2FA79 !important;
          border: 2px solid #853D03;
          color: #000 !important;
          transition: all 0.2s linear;
          border:none !important;
          }
          
          .choko-banner p {
          color: #fff;
          font-size: 25px;
          }
          
          .choko-banner .img img {
          object-fit: cover;
          width: 100%;
          max-width: 265px;
          }
          
          
          
          
          
          
          @keyframes bannerbouns {
          0% {
          
          }
          
          50% {
          transform: translateY(-10%);
          }
          
          100% {
          
          }
          }
          
